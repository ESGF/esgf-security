<?xml version="1.0" encoding="UTF-8"?>
<saml2:Assertion Version="2.0"
	xmlns:saml2="urn:oasis:names:tc:SAML:2.0:assertion">
	<saml2:Issuer
		Format="urn:oasis:names:tc:SAML:1.1:nameid-format:X509SubjectName">Test SAML Issuer</saml2:Issuer>
	<saml2:Subject>
		<saml2:NameID Format="urn:esg:openid">Test Openid</saml2:NameID>
	</saml2:Subject>
	<saml2:AuthnStatement>
		<saml2:AuthnContext>
			<saml2:AuthnContextClassRef>urn:oasis:names:tc:SAML:2.0:ac:classes:X509
			</saml2:AuthnContextClassRef>
		</saml2:AuthnContext>
	</saml2:AuthnStatement>
</saml2:Assertion>
